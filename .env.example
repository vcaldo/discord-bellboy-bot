# Discord Bot Token
DISCORD_TOKEN=your_bot_token_here

# Optional: Discord Guild ID (Server ID) - leave empty to log all servers
GUILD_ID=

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Auto join busiest voice channel (true/false)
AUTO_JOIN_BUSIEST=true

# Auto leave empty voice channel (true/false)
AUTO_LEAVE_EMPTY=true

# Coqui TTS Configuration
TTS_MODEL=tts_models/en/ljspeech/fast_pitch
TTS_CACHE_SIZE=50

# Available TTS Models (examples):
# Fast models:
# - tts_models/en/ljspeech/fast_pitch (default, fast and good quality)
# - tts_models/en/ljspeech/glow-tts

# High quality models (slower):
# - tts_models/en/ljspeech/tacotron2-DDC
# - tts_models/en/ljspeech/vits

# Multi-speaker models:
# - tts_models/en/vctk/vits
# - tts_models/en/sam/tacotron-DDC

# Other languages:
# - tts_models/es/mai/tacotron2-DDC (Spanish)
# - tts_models/fr/mai/tacotron2-DDC (French)
# - tts_models/de/thorsten/tacotron2-DDC (German)
